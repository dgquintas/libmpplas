<!DOCTYPE UI><UI version="3.0" stdsetdef="1">
<class>GuiProjectModulesBase</class>
<widget class="QWidget">
    <property name="name">
        <cstring>GuiProjectModulesBase</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>384</width>
            <height>362</height>
        </rect>
    </property>
    <property name="caption">
        <string>GuiProjectModulesBase</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>0</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QFrame">
            <property name="name">
                <cstring>mpModuleDefinitionFrame</cstring>
            </property>
            <property name="frameShape">
                <enum>Box</enum>
            </property>
            <property name="frameShadow">
                <enum>Sunken</enum>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>mpModulePathLabel</cstring>
                    </property>
                    <property name="text">
                        <string>Select the module &amp;path</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>mpModulePathLineEdit</cstring>
                    </property>
                </widget>
                <widget class="QLayoutWidget">
                    <property name="name">
                        <cstring>mpModulePathHBox</cstring>
                    </property>
                    <hbox>
                        <property name="name">
                            <cstring>unnamed</cstring>
                        </property>
                        <property name="margin">
                            <number>0</number>
                        </property>
                        <property name="spacing">
                            <number>6</number>
                        </property>
                        <widget class="QLineEdit">
                            <property name="name">
                                <cstring>mpModulePathLineEdit</cstring>
                            </property>
                            <property name="whatsThis" stdset="0">
                                <string>The absolute path of a module file.</string>
                            </property>
                        </widget>
                        <widget class="QPushButton">
                            <property name="name">
                                <cstring>mpModulePathPushButton</cstring>
                            </property>
                            <property name="text">
                                <string>Browse...</string>
                            </property>
                            <property name="whatsThis" stdset="0">
                                <string>Look for the module file whose absolute path will replace the content of text entry field.</string>
                            </property>
                        </widget>
                    </hbox>
                </widget>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>mpSourcePathLabel</cstring>
                    </property>
                    <property name="text">
                        <string>Select the &amp;source path</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>mpSourcePathLineEdit</cstring>
                    </property>
                </widget>
                <widget class="QLayoutWidget">
                    <property name="name">
                        <cstring>mpSourcePathHBox</cstring>
                    </property>
                    <hbox>
                        <property name="name">
                            <cstring>unnamed</cstring>
                        </property>
                        <property name="margin">
                            <number>0</number>
                        </property>
                        <property name="spacing">
                            <number>6</number>
                        </property>
                        <widget class="QLineEdit">
                            <property name="name">
                                <cstring>mpSourcePathLineEdit</cstring>
                            </property>
                            <property name="whatsThis" stdset="0">
                                <string>The absolute path of the base directory where the sources of the module reside.</string>
                            </property>
                        </widget>
                        <widget class="QPushButton">
                            <property name="name">
                                <cstring>mpSourcePathPushButton</cstring>
                            </property>
                            <property name="text">
                                <string>Browse...</string>
                            </property>
                            <property name="whatsThis" stdset="0">
                                <string>Look for the base directory where the sources of this module are stored. The absolute path will replace the content of text entry field.</string>
                            </property>
                        </widget>
                    </hbox>
                </widget>
            </vbox>
        </widget>
        <widget class="QLayoutWidget">
            <property name="name">
                <cstring>mpModuleButtonsHBox</cstring>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>0</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>mpAddPushButton</cstring>
                    </property>
                    <property name="enabled">
                        <bool>false</bool>
                    </property>
                    <property name="text">
                        <string>&amp;Add</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Add the above module definition to the list of modules that are part of this project.</string>
                    </property>
                </widget>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>mpModifyPushButton</cstring>
                    </property>
                    <property name="enabled">
                        <bool>false</bool>
                    </property>
                    <property name="text">
                        <string>&amp;Modify</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Modify the definition of the module by the new content of the text entries above.</string>
                    </property>
                </widget>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>mpRemovePushButton</cstring>
                    </property>
                    <property name="enabled">
                        <bool>false</bool>
                    </property>
                    <property name="text">
                        <string>&amp;Remove</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Remove the selected module definition from the project.</string>
                    </property>
                </widget>
            </hbox>
        </widget>
        <widget class="QListView">
            <column>
                <property name="text">
                    <string>Module path</string>
                </property>
                <property name="clickable">
                    <bool>false</bool>
                </property>
                <property name="resizeable">
                    <bool>true</bool>
                </property>
            </column>
            <column>
                <property name="text">
                    <string>Source path</string>
                </property>
                <property name="clickable">
                    <bool>false</bool>
                </property>
                <property name="resizeable">
                    <bool>true</bool>
                </property>
            </column>
            <property name="name">
                <cstring>mpModulesListView</cstring>
            </property>
            <property name="resizePolicy">
                <enum>AutoOneFit</enum>
            </property>
            <property name="allColumnsShowFocus">
                <bool>true</bool>
            </property>
            <property name="resizeMode">
                <enum>AllColumns</enum>
            </property>
            <property name="whatsThis" stdset="0">
                <string>The modules that are part of the project.</string>
            </property>
        </widget>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>mpModulePathLineEdit</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>GuiProjectModulesBase</receiver>
        <slot>moduleDefinitionChanged()</slot>
    </connection>
    <connection>
        <sender>mpSourcePathLineEdit</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>GuiProjectModulesBase</receiver>
        <slot>moduleDefinitionChanged()</slot>
    </connection>
    <connection>
        <sender>mpModulePathPushButton</sender>
        <signal>clicked()</signal>
        <receiver>GuiProjectModulesBase</receiver>
        <slot>selectModulePath()</slot>
    </connection>
    <connection>
        <sender>mpSourcePathPushButton</sender>
        <signal>clicked()</signal>
        <receiver>GuiProjectModulesBase</receiver>
        <slot>selectSourcePath()</slot>
    </connection>
    <connection>
        <sender>mpAddPushButton</sender>
        <signal>clicked()</signal>
        <receiver>GuiProjectModulesBase</receiver>
        <slot>addModule()</slot>
    </connection>
    <connection>
        <sender>mpModifyPushButton</sender>
        <signal>clicked()</signal>
        <receiver>GuiProjectModulesBase</receiver>
        <slot>modifyModule()</slot>
    </connection>
    <connection>
        <sender>mpModulesListView</sender>
        <signal>selectionChanged(QListViewItem*)</signal>
        <receiver>GuiProjectModulesBase</receiver>
        <slot>moduleSelected(QListViewItem*)</slot>
    </connection>
    <connection>
        <sender>mpRemovePushButton</sender>
        <signal>clicked()</signal>
        <receiver>GuiProjectModulesBase</receiver>
        <slot>removeModule()</slot>
    </connection>
</connections>
<slots>
    <slot access="protected">addModule()</slot>
    <slot access="protected">modifyModule()</slot>
    <slot access="protected">moduleDefinitionChanged()</slot>
    <slot access="protected">moduleSelected(QListViewItem*)</slot>
    <slot access="protected">removeModule()</slot>
    <slot access="protected">selectModulePath()</slot>
    <slot access="protected">selectSourcePath()</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>

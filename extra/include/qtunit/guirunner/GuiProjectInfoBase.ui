<!DOCTYPE UI><UI version="3.0" stdsetdef="1">
<class>GuiProjectInfoBase</class>
<widget class="QWidget">
    <property name="name">
        <cstring>GuiProjectInfoBase</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>253</width>
            <height>250</height>
        </rect>
    </property>
    <property name="caption">
        <string>GuiProjectInfoBase</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>0</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QLabel">
            <property name="name">
                <cstring>mpProjectNameLabel</cstring>
            </property>
            <property name="text">
                <string>Enter the &amp;name of the project :</string>
            </property>
            <property name="buddy" stdset="0">
                <cstring>mpProjectNameLineEdit</cstring>
            </property>
        </widget>
        <widget class="QLineEdit">
            <property name="name">
                <cstring>mpProjectNameLineEdit</cstring>
            </property>
            <property name="whatsThis" stdset="0">
                <string>The name that will be used to identify this project.</string>
            </property>
        </widget>
        <widget class="QLabel">
            <property name="name">
                <cstring>mpProjectAuthorLabel</cstring>
            </property>
            <property name="text">
                <string>Enter the &amp;author of the project :</string>
            </property>
            <property name="buddy" stdset="0">
                <cstring>mpProjectAuthorLineEdit</cstring>
            </property>
        </widget>
        <widget class="QLineEdit">
            <property name="name">
                <cstring>mpProjectAuthorLineEdit</cstring>
            </property>
            <property name="whatsThis" stdset="0">
                <string>The developer that is resposible for this project.</string>
            </property>
        </widget>
        <widget class="QLabel">
            <property name="name">
                <cstring>mpProjectDescriptionLabel</cstring>
            </property>
            <property name="text">
                <string>Enter a &amp;description about the project :</string>
            </property>
            <property name="buddy" stdset="0">
                <cstring>mpProjectDescriptionTextEdit</cstring>
            </property>
        </widget>
        <widget class="QTextBrowser">
            <property name="name">
                <cstring>mpProjectDescriptionTextEdit</cstring>
            </property>
            <property name="textFormat">
                <enum>PlainText</enum>
            </property>
            <property name="readOnly">
                <bool>false</bool>
            </property>
            <property name="whatsThis" stdset="0">
                <string>A description of the project.</string>
            </property>
        </widget>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>mpProjectNameLineEdit</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>GuiProjectInfoBase</receiver>
        <slot>nameChanged(const QString&amp;)</slot>
    </connection>
</connections>
<slots>
    <slot>nameChanged(const QString &amp;)</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
